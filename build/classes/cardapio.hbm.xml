<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sisar.modelo.Cardapio" table="cardapio">
		<id name="idCardapio" type="int" column="idcardapio">
			<generator class="increment" />
		</id>

		<property name="data">
			<column name="data" />
		</property>

		<property name="turno">
			<column name="turno" />
		</property>
	</class>
</hibernate-mapping>